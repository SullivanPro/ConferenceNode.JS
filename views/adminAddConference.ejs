<%- include('header') -%>
<main role="main" class="container" style="margin-top:6rem">

    <div class="row">
        <div class="col-md-12">
            <h1>Ajouter une conférence</h1>
            <form method="POST" action="/admin/addConference">
                <div class="form-group">
                    <label for="titleConference">Titre</label>
                    <input type="text" name="title" class="form-control" id="titleConference" required>
                </div>
                <div class="form-group">
                    <label for="videoConference">Lien vidéo</label>
                    <input type="text" class="form-control" id="videoConference" name="video" required>
                </div>
                <button type="submit" class="btn btn-primary">Ajouter</button>
            </form>
        </div>
    </div>

</main><!-- /.container -->
<%- include('footer') -%>